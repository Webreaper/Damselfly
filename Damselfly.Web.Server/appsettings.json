{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DamselflyConfiguration": {
    "SourceDirectory": "PathToFiles",
    "ThumbPath": "PathToFiles",
    "DatabasePath": "PathToDB",
    "LogPath": "PathToLogs",
    "DownloadPath": "PathToTemporaryLocationForDownloads",
    "Verbose": false,
    "Trace": false,
    "Readonly": false,
    "Port": 6363,
    "Synology": false,
    "NoGenerateThumbnails": false,
    "EnableIndexing": false,
    "UsePostgresDB": false
  },
  "ImageMagick": {
    "ExePath": "PathToExe"
  },
  "ExifTool": {
    "ExePath": "PathToExe"
  },
  "MailGun": {
    "Domain": "mailgunDomain",
    "ApiKey": "MailGunApiKey",
    "FromAddress": "FromAddressForMailGun"
  },
  "ContactForm": {
    "ToAddress": "emailAddressToSendContactRequests"
  },
  "Google": {
    "ClientId": "YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com",
    "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET",
    "RedirectUri": "https://yourdomain.com/api/google/callback",
    "ApplicationName": "YOUR_APPLICATION_NAME"
  },
  "jwt": {
    "Firebase": {
      "ValidIssuer": "https://securetoken.google.com/{FIREBASEID}",
      "ValidAudience": "FIREBASEID"
    }
  },
  "AdminEmails": "CommaSeperatedListOfAdmins",
  "Encryption": {
    "Key": "YourEncryptionKeyHere",
    "IV": "YourInitializationVectorHere"
  }
}
